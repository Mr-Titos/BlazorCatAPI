@page "/home"
@* @attribute [Authorize] *@

@inject CatService CatService
@inject LoginService LoginService
@inject NavigationManager NavigationManager

<PageTitle>Home</PageTitle>

<CatController cat="CatSelected"></CatController>

@code {
    private Cat CatSelected { get; set; }

    protected override void OnInitialized()
    {
        CatSelected = Task.Run(() => CatService.GetCatImage()).Result ?? throw new Exception("No cat retrieved");
    }

}