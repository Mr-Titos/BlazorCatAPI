@page "/home"

@inject CatService CatService
@inject NavigationManager NavigationManager

<PageTitle>Home</PageTitle>

<CatController Cat="CatSelected"></CatController>

@code {
    private Cat CatSelected { get; set; }

    protected override void OnInitialized()
    {
        CatSelected = Task.Run(() => CatService.SearchCat()).Result ?? throw new Exception("No cat retrieved");
    }

}